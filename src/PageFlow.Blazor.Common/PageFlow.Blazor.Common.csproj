<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net9.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>PageFlow.Blazor.Common</Title>
		<Authors>Daniel Mendie</Authors>
		<Description>Contains decorators, helpers and utilities for DynamicNavigation</Description>
		<Copyright>2024</Copyright>
		<PackageProjectUrl>https://www.nuget.org/packages/PageFlow.Blazor.Common</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/danielmendie/PageFlow</RepositoryUrl>
		<PackageTags>Library</PackageTags>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>2.0.1</Version>
		<PackageIcon>icon.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\..\icon.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\..\LICENSE">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
